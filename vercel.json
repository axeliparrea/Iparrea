{
    "version": 2,
    "builds": [
        {
            "src": "frontend/package.json",
            "use": "@vercel/static-build",
            "config": { "distDir": "dist" }
        }
    ],
    "routes": [
        { "src": "/static/(.*)", "dest": "/static/$1" },
        { "src": "/assets/(.*)", "dest": "/assets/$1" },
        { "src": "/favicon.png", "dest": "/favicon.png" },
        { "src": "/cv-axel-iparrea.pdf", "dest": "/cv-axel-iparrea.pdf" },
        { "src": "/Axel_Iparrea_ComputerScience.pdf", "dest": "/Axel_Iparrea_ComputerScience.pdf" },
        { "src": "/(.*)", "dest": "/index.html" }
    ]
} 